<!DOCTYPE HTML>
<HTML>

<HEAD>
    <TITLE>REGISTRATION FORM</TITLE>
</HEAD>

<BODY>
    <style>
        div {
            width: 500px;
            height: 1200px;
            padding: 30px;
            margin-left: 10%;
            background-color: #A92ACC;
            size: 20px;
            border: rgb(0, 162, 255);
            border-style: solid;
            border-width: 2px;
        }
        
        body {
            text-align: center;
            background-image: url('data:image/jpeg;base64,');
            background-position-x: left;
            background-attachment: fixed;
            background-size: cover;
            font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
            font-style: normal;
            font-variant: normal;
            font-size: medium;
            font-weight: normal;
        }
    </style>
    <center>
        <div>
            <form name="FORMVAL" action="#" method="POST" onsubmit="return validateform()">
                <H1 style="font-size:200%">REGISTRATION FORM</H1>
                <HR STYLE="height:2px;border-width:0;color:gray;background-color:black">
                <table>
                    <tr>
                        <td colspan="2" width="100%">
                            <h2>
                                <center>:ENTER YOUR PERSONAL DETAILS: </center>
                            </h2>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <LABEL>Username:</LABEL><br><br>
                            <INPUT type="text" name="username">
                        </td>

                        <td><br>
                            <LABEL>Password :</LABEL><br><br>
                            <INPUT type="password" name="password"><br>
                            <BR>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            GENDER:
                        </td>
                        <td><br>
                            <INPUT type="RADIO" id="MALE" name="GENDER" value="MALE">
                            <LABEL>MALE</LABEL>
                            <INPUT type="RADIO" id="FEMALE" name="GENDER" value="FEMALE">
                            <LABEL>FEMALE</LABEL>
                            <INPUT type="RADIO" id="OTHER" name="GENDER" value="OTHER">
                            <LABEL>OTHER</LABEL><br><br>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <LABEL>DOB :</LABEL><br><br>
                            <input type="date" id="birthday" name="birthday">
                        </td>
                        <td width="50%">
                            <LABEL>AGE :</LABEL><br><br>
                            <INPUT type="text" name="age">
                        </td>
                    </tr>
                    <tr>
                        <td width="50%"><br><br>
                            <LABEL>EMAIL :</LABEL><br><br>
                            <INPUT type="text" name="email">
                        </td>
                        <td width="50%"><br><br>
                            <LABEL>CONTACT :</LABEL><br><br>
                            <INPUT type="numeric" name="contact">
                        </td>
                    </tr>
                    <tr>
                        <td width="50%"><br><br>
                            <LABEL>CATEGORY :</LABEL><br><br>
                            <select id="CATEGORY">
    <option value="A+VE">NONE</OPTION>
    <option value="GENERAL">GENERAL</OPTION>
    <option value="OBC">OBC</OPTION>
    <option value="SC/ST">SC/ST</OPTION>
    <option value="NRI">NRI</OPTION>
    </SELECT>
                        </td>
                        <td width="50%"><br><br>
                            <label>BLOOD GROUP :</label><br><br>
                            <select id="GROUP">
    <option>A+ve</OPTION>
    <option>A-ve</OPTION>
    <option>B+ve</OPTION>
    <option>B-ve</OPTION>
    <option>O+ve</OPTION>
    <option>O-ve</OPTION>
    <option>AB+ve</OPTION>
    <option>AB-ve</OPTION>
    </SELECT>

                        </td>
                    </tr>
                </table>
                <br><br>
                <HR STYLE="height:2px;border-width:0;color:gray;background-color:black">
                <table width="50%"><br>
                    <tr>
                        <td colspan="2">
                            <H2>
                                <center>: ENTER ADDRESS DETAILS :</center>
                            </H2>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2">

                            <LABEL>ADDRESS :<br></LABEL><br>
                            <TEXTAREA id="ADDRESS" name="ADDRESS" rows="2" cols="50"></TEXTAREA>
                            <BR>
                            <BR>
                        </td>
                    </tr>
                    <tr>
                        <td width="50%">
                            <LABEL>CITY :</LABEL><br><br>
                            <INPUT type="text" name="city">
                        </td>
                        <td>
                            <LABEL>PINCODE :</LABEL><br><br>
                            <INPUT type="text">
                            <BR>

                        </td>
                    </tr>
                    <tr>
                        <center>
                            <td width="50%">
                                <LABEL>STATE :</LABEL><br><br>
                                <INPUT type="text" name="state">
                            </td>
                            <td><br><br>
                                <LABEL>COUNTRY :</LABEL><br><br>
                                <INPUT type="text" name="country">
                                <BR>
                                <BR><br>
                        </center>
                        </td>
                    </tr>
                </table>
                <HR STYLE="height:2.5px;border-width:0;color:gray;background-color:black">
                <br><br><input style="background-color: lightpink; width: 300px; height: 30px;" type="submit" />
            </form>
        </div>
        </form>
        <script>
            function validateform() {
                var name = document.forms["FORMVAL"]["username"];
                var password = document.forms["FORMVAL"]["password"];
                var age = document.forms["FORMVAL"]["age"];
                var email = document.forms["FORMVAL"]["email"];
                var contact = document.forms["FORMVAL"]["contact"];
                var city = document.forms["FORMVAL"]["city"];
                var state = document.forms["FORMVAL"]["state"];
                var country = document.forms["FORMVAL"]["country"];
                if (name.value == "") {
                    window.alert("Please enter appropriate username");
                    name.focus();
                    return false;
                }
                if (password.value.length < 10) {
                    window.alert("Error : Minimum length of password is 10!");
                    password.focus();
                    return false;
                }
                if (age.value == "" || age.value < 1 || age.value > 60) {
                    window.alert("Pl enter correct age");
                    age.focus();
                    return false;
                }
                if (email.value == "") {
                    window.alert("Please enter correct email id");
                    email.focus();
                    return false;
                }
                if (email.value.indexOf("@", 0) < 0) {
                    window.alert("Please enter a valid e-mail address.");
                    email.focus();
                    return false;
                }
                if (email.value.indexOf(".", 0) < 0) {
                    window.alert("Please enter a valid e-mail address.");
                    email.focus();
                    return false;
                }
                if (contact.value.length != 10) {
                    window.alert("Error : Pl enter valid contact no.");
                    contact.focus();
                    return false;
                }
                if (city.value == "") {
                    window.alert("Pl enter right city");
                    city.focus();
                    return false;
                }
                if (state.value == "") {
                    window.alert("Pl enter right state name");
                    state.focus();
                    return false;
                }
                if (country.value == "") {
                    window.alert("Pl enter right country name");
                    state.focus();
                    return false;
                }
                return true;
            }
        </script>
    </center>
</BODY>

</HTML>